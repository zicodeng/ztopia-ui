{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": ["@babel/plugin-transform-runtime"],
    "env": {
        "production": {
            "plugins": [["emotion", { "hoist": true }]]
        },
        "development": {
            "plugins": [
                ["emotion", { "sourceMap": true, "autoLabel": true }]
            ]
        }
    }
}